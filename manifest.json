{
	"manifest_version": 2,
	"name": "Element Remover",
	"description": "Allows removing elements from webpages",
	"version": "1.0.0",
	"icons": {},
	"permissions": ["<all_urls>", 
	"menus",
	"activeTab"],
	"background": {
		"page": "src/background/index.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/content/index.ts"
			]
		}
	]
}