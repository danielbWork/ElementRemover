{
	"manifest_version": 2,
	"name": "PageEditor",
	"description": "Allows editing webpages without opening dev menu",
	"version": "1.0.0",
	"icons": {},
	"permissions": ["<all_urls>", 
	"menus",
	"activeTab"],
	"background": {
		"page": "src/background/index.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/content/index.ts"
			]
		}
	],
	"browser_action": {
		"default_popup": "src/browserAction/index.html",
		"default_title": "PageEditor"
	}
}